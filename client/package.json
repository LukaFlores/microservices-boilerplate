{
  "name": "fullstack-k8s-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "axios": "^0.26.1",
    "concurrently": "^7.1.0",
    "react": "16.10.0",
    "react-dom": "16.10.0",
    "react-on-images-loaded": "^2.2.7",
    "react-reveal": "^1.2.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "start": "concurrently \"npm run start:css\" \"react-scripts start\"",
    "dev": " npm run dev:css && react-scripts start ",
    "start:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./dist/output.css --watch",
    "dev:css": "tailwindcss -o src/styles/tailwind.css",
    "build": "npm run build:css &&  GENERATE_SOURCEMAP=false CI=false react-scripts --max_old_space_size=4096 build",
    "build:css": "NODE_ENV=production tailwindcss -o src/styles/tailwind.css -m"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "presets": [
    "@babel/preset-env"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "tailwindcss": "^3.0.24"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
